eye "$@" --wcache http://eyereasoner.github.io/eye/reasoning .. http://eyereasoner.github.io/eye/reasoning/socrates-metachain-f2b/metachain.n3 http://eyereasoner.github.io/eye/reasoning/socrates-metachain-f2b/socrates.n3 --query http://eyereasoner.github.io/eye/reasoning/socrates-metachain-f2b/socrates-query.n3 --output socrates-metachain-proof.n3
