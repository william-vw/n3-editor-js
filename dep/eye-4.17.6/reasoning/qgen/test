eye "$@" --wcache http://eyereasoner.github.io/eye/reasoning .. --nope http://eyereasoner.github.io/eye/reasoning/socrates/socrates.n3 --query http://eyereasoner.github.io/eye/reasoning/qgen/query.n3 --output answer.n3
