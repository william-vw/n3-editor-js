@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/test#>.
@prefix n3: <https://www.w3.org/ns/n3#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix e: <http://eulersharp.sourceforge.net/2003/03swap/log-rules#>.
@prefix var: <http://eyereasoner.github.io/var#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :task1 a :AllTasksCompleted.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :task1 a :AllTasksCompleted.
    };
    r:evidence (
        skolem:lemma2
        skolem:lemma3
    );
    r:binding [ r:variable [ <http://www.w3.org/2004/06/rei#uri> "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ <http://www.w3.org/2004/06/rei#uri> "http://example.org/test#task1"]];
    r:binding [ r:variable [ <http://www.w3.org/2004/06/rei#uri> "http://eyereasoner.github.io/var#x_1"]; r:boundTo [ a r:Existential; <http://www.w3.org/2004/06/rei#nodeId> "_:sk_0"]];
    r:rule skolem:lemma4.

skolem:lemma2 a r:Extraction;
    r:gives {
        :task1 a :CompositeTask.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/n3c/tasks.n3>].

skolem:lemma3 a r:Inference;
    r:gives {
        ({
            :task1 :subTask _:sk_0.
        } {
            _:sk_0 a :Completed.
        }) n3:forAllIn <http://eyereasoner.github.io/eye/reasoning/n3c/subtasks.n3>.
    };
    r:evidence (
        [ a r:Fact; r:gives {<http://eyereasoner.github.io/eye/reasoning/n3c/subtasks.n3> log:semantics {
            :task1 :subTask :subtask1.
            :subtask1 a :Completed.
            :task1 :subTask :subtask2.
            :subtask2 a :Completed.
            :task1 :subTask :subtask3.
            :subtask3 a :Completed.
            :task2 :subTask :subtask4.
            :subtask4 a :Completed.
            :task2 :subTask :subtask5.
        }}]
        [ a r:Fact; r:gives {("forall" {
            {
                :task1 :subTask :subtask1.
                :subtask1 a :Completed.
                :task1 :subTask :subtask2.
                :subtask2 a :Completed.
                :task1 :subTask :subtask3.
                :subtask3 a :Completed.
                :task2 :subTask :subtask4.
                :subtask4 a :Completed.
                :task2 :subTask :subtask5.
            } log:includes {
                :task1 :subTask _:sk_0.
            }.
        } {
            {
                :task1 :subTask :subtask1.
                :subtask1 a :Completed.
                :task1 :subTask :subtask2.
                :subtask2 a :Completed.
                :task1 :subTask :subtask3.
                :subtask3 a :Completed.
                :task2 :subTask :subtask4.
                :subtask4 a :Completed.
                :task2 :subTask :subtask5.
            } log:includes {
                _:sk_0 a :Completed.
            }.
        }) e:derive true}]
    );
    r:binding [ r:variable [ <http://www.w3.org/2004/06/rei#uri> "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ <http://www.w3.org/2004/06/rei#uri> "http://eyereasoner.github.io/eye/reasoning/n3c/subtasks.n3"]];
    r:binding [ r:variable [ <http://www.w3.org/2004/06/rei#uri> "http://eyereasoner.github.io/var#x_1"]; r:boundTo {
        :task1 :subTask :subtask1.
        :subtask1 a :Completed.
        :task1 :subTask :subtask2.
        :subtask2 a :Completed.
        :task1 :subTask :subtask3.
        :subtask3 a :Completed.
        :task2 :subTask :subtask4.
        :subtask4 a :Completed.
        :task2 :subTask :subtask5.
    }];
    r:binding [ r:variable [ <http://www.w3.org/2004/06/rei#uri> "http://eyereasoner.github.io/var#x_2"]; r:boundTo {
        :task1 :subTask _:sk_0.
    }];
    r:binding [ r:variable [ <http://www.w3.org/2004/06/rei#uri> "http://eyereasoner.github.io/var#x_3"]; r:boundTo {
        _:sk_0 a :Completed.
    }];
    r:rule skolem:lemma5.

skolem:lemma4 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 a :CompositeTask.
            ({
                var:x_0 :subTask var:x_1.
            } {
                var:x_1 a :Completed.
            }) n3:forAllIn <http://eyereasoner.github.io/eye/reasoning/n3c/subtasks.n3>.
        } => {
            var:x_0 a :AllTasksCompleted.
        }.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/n3c/forAllIn_query.n3>].

skolem:lemma5 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1, var:x_2, var:x_3. {
            (var:x_2 var:x_3) n3:forAllIn var:x_0.
        } <= {
            var:x_0 log:semantics var:x_1.
            ("forall" {
                var:x_1 log:includes var:x_2.
            } {
                var:x_1 log:includes var:x_3.
            }) e:derive true.
        }.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/n3c/components.n3>].

