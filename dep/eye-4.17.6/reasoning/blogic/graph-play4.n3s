@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

() log:onNegativeSurface {
    (_:E) log:onNegativeSurface {
        _:E a :C.
    }.
}.

(_:a _:b _:S1) log:onNegativeSurface {

    _:a log:onNegativeSurface {
        _:b log:onNegativeSurface _:S1.
    }.

    () log:onNegativeSurface {
        () log:onNegativeSurface _:S1.
    }.
}.

(_:s _:p _:o) log:onQuerySurface {
    () log:onNegativeSurface {
        _:s _:p _:o.
    }.
}.
