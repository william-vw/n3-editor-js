@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

(_:S1 _:S2) log:onNegativeSurface {

    _:S1 log:equalTo {
        (_:V) log:onNegativeSurface {
            _:V a :C .
        } .
    } .

    _:S2 log:equalTo {
        (_:E) log:onNegativeSurface {
            _:E a :C .
        } .
    } .

    _:S1 log:isomorphic _:S2 .

    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onQuerySurface {
    :test :is true .
} .
