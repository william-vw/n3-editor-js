#!/bin/bash -x
eye "$@" --wcache http://eyereasoner.github.io/eye/reasoning .. --nope http://eyereasoner.github.io/eye/reasoning/polygon/polygon.n3 --query http://eyereasoner.github.io/eye/reasoning/polygon/polygon_query.n3 --output polygon_answer.n3
eye "$@" --wcache http://eyereasoner.github.io/eye/reasoning .. http://eyereasoner.github.io/eye/reasoning/polygon/polygon.n3 --query http://eyereasoner.github.io/eye/reasoning/polygon/polygon_query.n3 --output polygon_proof.n3
